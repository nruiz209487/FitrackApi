                        "description": "ID del usuario",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "routine_id",
                        "in": "path",
                        "description": "ID de la rutina",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Rutina eliminada exitosamente"
                    },
                    "404": {
                        "description": "Rutina no encontrada"
