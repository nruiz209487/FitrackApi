        "/notes/user/{user_id}/{id}": {
            "delete": {
                "tags": [
                    "Notes"
                ],
                "summary": "Delete a note by user ID and note ID",
                "operationId": "af7467d40d00ac472fe58d1d324e2d3e",
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "path",
                        "description": "ID of the user",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the note",
                        "required": true,
                        "schema": {
