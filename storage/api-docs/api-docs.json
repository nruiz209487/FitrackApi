    },
    "paths": {
        "/api/exercises": {
            "get": {
                "tags": [
                    "exercises"
                ],
                "summary": "Obtener lista de exercises",
                "operationId": "be2d98c446d6fd5505b280c573831621",
                "responses": {
                    "200": {
                        "description": "Lista de exercises"
                    }
                }
            }
        },
        "/exercise-log/{user_id}": {
            "get": {
                "tags": [
                    "ExerciseLog"
                ],
                "summary": "Obtener logs de ejercicio por ID de usuario",
                "operationId": "getExerciseLogsByUserId",
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de logs del usuario"
                    }
                }
            },
            "post": {
                "tags": [
                    "ExerciseLog"
                ],
                "summary": "Insertar un nuevo log de ejercicio para un usuario",
                "operationId": "insertExerciseLogByUserId",
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "id"
                                ],
                                "properties": {
                                    "id": {
                                        "type": "integer",
                                        "example": 1
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Log insertado exitosamente"
                    },
                    "200": {
                        "description": "El log ya existe para ese usuario"
                    }
                }
            }
        },
        "/exercise-log/{user_id}/{exercise_Id}": {
            "delete": {
                "tags": [
                    "ExerciseLog"
                ],
                "summary": "Eliminar un log de ejercicio por ID de usuario e ID de ejercicio",
                "operationId": "deleteExerciseLogByUserIdAndExerciseId",
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "exercise_Id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Log eliminado exitosamente"
                    },
                    "404": {
                        "description": "Log no encontrado"
                    }
                }
            }
        },
        "/notes/user/{user_id}": {
